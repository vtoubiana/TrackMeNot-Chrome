{
    "author": "TrackMeNot",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "32": "tmn.png"
        },
        "default_popup": "tmn_menu.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "tmn_search.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "description": "Protect against data profiling by search engines",
    "icons": {
        "16": "icon16.png",
        "48": "icon32.png"
    },
    "manifest_version": 2,
    "name": "TrackMeNot",
    "options_page": "options.html",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.google.com/",
        "tabs",
        "https://*/*",
        "http://*/*",
        "webNavigation",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "version": "0.10.3",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}